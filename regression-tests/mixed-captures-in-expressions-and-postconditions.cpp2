
#include <algorithm>
#include <vector>

main: () -> int = {
    vec: std::vector<std::string>
            = ("hello", "2022");

    y: std::string = "\n";
    callback := :(x) = std::cout << x << y&$*;

    std::ranges::for_each( vec, callback );
    y = "-ish\n";
    std::ranges::for_each( vec, callback );
}

vec: std::vector<int> = ();

insert_at: (where: int, val: int)
    pre( 0 <= where && where <= vec.ssize() )
    post( vec.ssize() == vec.ssize()$ + 1 )
= {
    _ = vec.insert( vec.begin()+where, val );
}
