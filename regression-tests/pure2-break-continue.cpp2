do_continue_inner: () =
{
    i := 0;
    do {
        j := 0;
        inner: do {
            std::cout << i << j << " ";
            if j == 1 {
                continue inner;
            }
            std::cout << "inner ";
        }
        while j<3 next j++;

        std::cout << "outer ";
    }
    while i<3 next i++;
}

