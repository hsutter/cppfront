#include <iostream>
#include <string_view>

const char carr[] = "carray test";

main: () -> int = {
    a := 2;
    b: std::optional<int> = ();
    std::cout << "a = (a)$, b = (b)$\n";

    b = 42;
    std::cout << "a^2 + b = (a * a + b.value())$\n";

    std::cout << "carr = (carr)$\n";    

    sv : std::string_view = "SV works!";
    std::cout << "sv = (sv)$\n";
  
    osv : std::optional<std::string_view> = ();
    std::cout << "osv = (osv)$\n";
    osv = "It works, too";
    std::cout << "osv = (osv)$\n";
}
