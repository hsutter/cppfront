#include <vector>
#include <ranges>
#include <string>
#include <span>
#include <algorithm>
#include <iostream>

main: () = {
    vec: std::vector<std::string> 
            = ("hello", "2022");
    view: std::span = vec;

    std::ranges::for_each
        ( view, :(x:_) = { std::cout << x << "\n"; } );

    callback := :(inout x:_) = x += "-ish";
    std::ranges::for_each( view, callback );

    for view do :(str:_) =
        std::cout << str << "\n";
}
