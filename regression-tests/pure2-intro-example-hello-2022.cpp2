main: () = {
    vec: std::vector<std::string> 
            = ("hello", "2022");
    view: std::span = vec;

    for view do :(inout str:_) = {
        len := decorate(str);
        println(str, len);
    }
}

decorate: (inout thing: _ ) -> int = { 
    thing = "[" + thing + "]";
    return thing.ssize();
}

println: (x: _, len: _) =
    std::cout
        << ">> " << x 
        << " - length " 
        << len << "\n";
