
main: () -> int = {
    v: std::vector<int> = (1, 2, 3, 4, 5);
    add_42_to_subrange(v, 1, 3);

    for v do :(i) =
        std::cout << i << "\n";
}

add_42_to_subrange: (inout rng:_, start:int, end:int)
= {
    [[assert Bounds: 0 <= start]]
    [[assert Bounds: end <= rng.ssize()]]

    count := 0;
    for  rng
    next count++
    do   :(inout i) =
        if start <= count <= end {
            i += 42;
        }
}

#include <vector>
#include <span>
#include <iostream>
