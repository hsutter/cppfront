
main: () = {
    v: std::vector<int> = (1, 2, 3, 4, 5);
    add_42_to_subrange(v, 1, 3);

    for v do: (i:_) =
        std::cout << i << "\n";
}

add_42_to_subrange: (inout rng:_, start:int, end:int)
= {
    [[assert Bounds: 0 <= start]]
    [[assert Bounds: end <= rng.size()]]

    count := 0;
    for  rng 
    next count++ 
    do:  (inout i:_) =
        if start <= count && count <= end {
            i += 42;
        }
}

#include <vector>
#include <span>
#include <iostream>
